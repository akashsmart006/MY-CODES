#include<bits/stdc++.h>
using namespace std;
	
int main()
{
	int n,m,a,r1,r2,c1,c2;
	cin>>n>>m;
	vector<int> v1;
	vector<vector<int>> v2( n , vector<int> (m, 0)),pref( n , vector<int> (m, 0));
	for(int i=0;i<n;i++)
	{
		for(int j=0;j<m;j++)
		{
			cin>>a;
			v2[i][j]=a;
		}
	}
	cin>>r1>>c1>>r2>>c2;
	pref[0][0]=v2[0][0];
	for(int i=1;i<n;i++)
	{
		pref[i][0]=pref[i-1][0]+v2[i][0];
	}
	for(int j=1;j<m;j++)
	{
		pref[0][j]=pref[0][j-1]+v2[0][j];
	}
	for(int i=1;i<n;i++)
	{
		for(int j=1;j<m;j++)
		{
			pref[i][j]=pref[i-1][j]+pref[i][j-1]-pref[i-1][j-1]+v2[i][j];
		}
	}
	for(int i=0;i<n;i++)
	{
		for(int j=0;j<m;j++)
		{
			cout<<pref[i][j]<<" ";
		}
		cout<<endl;
	}
	cout<<"ans :"<<endl;
	cout<<pref[r2][c2]-pref[r2][c1-1]-pref[r1-1][c2]+pref[r1-1][c1-1];
}
